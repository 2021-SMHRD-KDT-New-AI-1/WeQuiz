<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00BCD4"
    tools:context=".MissionListActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/quiz_back"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-121dp" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:layout_width="40dp"
        android:layout_height="40dp" />




    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dream5"
        android:text="펭귄마을에서 펭귄 찍기"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="191dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dream5"
        android:text="국립아시아문화전당(ACC)에서 분수대 찍기"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="365dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dream5"
        android:text="전일빌딩 관련 문제 풀기"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="533dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="273dp"
        android:layout_height="38dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/dream3"
        android:letterSpacing="0.1"
        android:text="양림동 펭귄마을(광주 남구 천변좌로446번길 7)로 이동하여 펭귄 사진을 찍어보세요"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintTop_toBottomOf="@+id/textView1"
        tools:layout_editor_absoluteX="35dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="273dp"
        android:layout_height="38dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/dream3"
        android:letterSpacing="0.1"
        android:text="국립아시아문화전당(ACC)(광주 동구 문화전당로 38)로 이동하여 분수대 사진을 찍어보세요!"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_editor_absoluteX="35dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="273dp"
        android:layout_height="38dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/dream3"
        android:letterSpacing="0.1"
        android:text="전일빌딩(광주광역시 동구 금남로 245)로 이동하여 전일빌딩 관련 문제를 풀어보세요!"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_editor_absoluteX="35dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="162dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="광주송정역"
        android:textColor="@color/white"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="27dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="166dp"
        android:layout_height="42dp"
        android:gravity="center"
        android:text="에서의 미션"
        android:textColor="@color/white"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="27dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="84dp"
        android:layout_height="28dp"
        android:layout_gravity="center"
        android:layout_marginTop="5dp"
        android:backgroundTint="#FF6D64"
        android:text="미션 수행하기"
        android:textSize="8sp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_editor_absoluteX="35dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="84dp"
        android:layout_height="28dp"
        android:layout_gravity="center"
        android:layout_marginTop="5dp"
        android:backgroundTint="#FF6D64"
        android:text="미션 수행하기"
        android:textSize="8sp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:layout_editor_absoluteX="35dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="84dp"
        android:layout_height="28dp"
        android:layout_gravity="center"
        android:layout_marginTop="5dp"
        android:backgroundTint="#FF6D64"
        android:text="미션 수행하기"
        android:textSize="8sp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_editor_absoluteX="35dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:fontFamily="@font/dream6"
        android:text="미션 생성하기"
        app:layout_constraintTop_toTopOf="@+id/guideline3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="50dp" />

</androidx.constraintlayout.widget.ConstraintLayout>