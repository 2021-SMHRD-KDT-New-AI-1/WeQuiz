<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignActivity">

    <Button

        android:id="@+id/button6"
        android:layout_width="50dp"
        android:layout_height="23dp"
        android:background="@drawable/button_color"
        android:fontFamily="@font/dream4"
        android:letterSpacing="-0.05"
        android:paddingLeft="0dp"
        android:paddingTop="0dp"
        android:paddingRight="0dp"
        android:paddingBottom="0dp"


        android:text="중복 확인"
        android:textColor="#606060"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/editText9"
        app:layout_constraintStart_toEndOf="@+id/editText9"
        app:layout_constraintTop_toTopOf="@+id/editText9" />

    <ImageView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/join_logo" />

    <TextView

        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dream4"
        android:text="아이디"

        android:textColor="#0C430E"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/editText6"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/editText6" />

    <EditText


        android:id="@+id/editText6"
        android:layout_width="190dp"
        android:layout_height="36dp"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="88dp"
        android:ems="10"
        android:fontFamily="@font/dream3"
        android:hint="영어 대문자, 소문자, 숫자를 조합"
        android:inputType="textPersonName"

        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button

        android:layout_width="50dp"
        android:layout_height="23dp"
        android:background="@drawable/button_color"
        android:fontFamily="@font/dream4"
        android:letterSpacing="-0.05"
        android:paddingLeft="0dp"
        android:paddingTop="0dp"
        android:paddingRight="0dp"
        android:paddingBottom="0dp"
        android:text="중복 확인"
        android:textColor="#606060"


        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/editText6"
        app:layout_constraintStart_toEndOf="@+id/editText6"
        app:layout_constraintTop_toTopOf="@+id/editText6" />

    <TextView

        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:fontFamily="@font/dream4"
        android:text="비밀번호"

        android:textColor="#0C430E"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/editText7"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/editText7"
        app:layout_constraintVertical_bias="0.555" />

    <EditText


        android:id="@+id/editText7"
        android:layout_width="240dp"
        android:layout_height="36dp"
        android:layout_marginTop="15dp"

        android:ems="10"
        android:hint="비밀번호를 입력해주세요"

        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/editText6"
        app:layout_constraintTop_toBottomOf="@+id/editText6" />

    <TextView

        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/dream4"
        android:text="비밀번호 확인"


        android:textColor="#0C430E"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/editText8"
        app:layout_constraintEnd_toStartOf="@+id/editText8"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/editText8" />

    <EditText


        android:id="@+id/editText8"
        android:layout_width="240dp"
        android:layout_height="36dp"
        android:layout_marginTop="15dp"

        android:ems="10"
        android:hint="비밀번호를 한 번 더 입력해주세요"

        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/editText7"
        app:layout_constraintTop_toBottomOf="@+id/editText7" />

    <TextView

        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dream4"
        android:text="닉네임"

        android:textColor="#0C430E"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/editText9"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/editText9" />

    <EditText

        android:id="@+id/editText9"
        android:layout_width="190dp"
        android:layout_height="36dp"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:hint="닉네임을 입력해주세요"

        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/editText8"
        app:layout_constraintTop_toBottomOf="@+id/editText8" />


    <TextView

        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dream4"
        android:text="생년월일"
        android:textColor="#0C430E"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <Button

        android:layout_width="50dp"
        android:layout_height="23dp"
        android:background="@drawable/button_color"
        android:fontFamily="@font/dream4"
        android:letterSpacing="-0.05"

        android:paddingLeft="0dp"
        android:paddingTop="0dp"
        android:paddingRight="0dp"
        android:paddingBottom="0dp"
        android:text="선택"
        android:textColor="#606060"
        android:textSize="10sp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.7" />

    <TextView

        android:id="@+id/textView2"
        android:layout_width="190dp"
        android:layout_height="33dp"
        android:gravity="center"
        android:text="날짜 정보"
        app:layout_constraintStart_toStartOf="@+id/editText9"
        app:layout_constraintTop_toBottomOf="@+id/editText9" />

    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dream4"
        android:text="성별"
        android:textColor="#0C430E"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/radioGroup"
        app:layout_constraintStart_toStartOf="@+id/guideline4" />

    <RadioGroup

        android:id="@+id/radioGroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <RadioButton

            android:layout_width="61dp"
            android:layout_height="23dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="24dp"
            android:text="여자" />

        <RadioButton

            android:layout_width="61dp"
            android:layout_height="23dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="120dp"
            android:text="남자" />

    </RadioGroup>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.90895295" />

    <Button

        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/gradation_color"
        android:fontFamily="@font/dream6"
        android:text="가입하기"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="28dp" />


</androidx.constraintlayout.widget.ConstraintLayout>