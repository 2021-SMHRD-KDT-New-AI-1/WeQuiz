<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/savemis_back"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".savemission"
    android:clickable="true">

    <ImageView
        android:id="@+id/btn_changemis"
        android:layout_width="252dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btn_savemis"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/btn_savemis"
        app:layout_constraintTop_toBottomOf="@+id/btn_savemis"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/changemission" />

    <ImageView
        android:id="@+id/btn_savemis"
        android:layout_width="252dp"
        android:layout_height="56dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@+id/missiontit"
        app:layout_constraintStart_toStartOf="@+id/missiontit"
        app:layout_constraintTop_toBottomOf="@+id/missiontit"
        app:srcCompat="@drawable/savemission" />

    <TextView
        android:id="@+id/missiontit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="100dp"
        android:fontFamily="@font/dream7"
        android:text="펭귄마을에서사진찍기"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_savemis"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>